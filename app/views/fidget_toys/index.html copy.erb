<h2>Hew, this is being hosted now!</h2>
<div class="row">
  <% @toys.each do |toy| %>
    <div class="col-md-4">
      <div class="thumbnail well">
        <h2><a href="/toys/<%= toy.id %>"><%= toy.name %></a></h2>
        <h4><%= toy.name %></h4>
        <h3><%= toy.image %></h3>
        <img src="<%= toy.image %>" alt="<%= toy.name %>">
        <h3><%= toy.description %></h3>
        <h3><%= toy.price %></h3>
      </div>
    </div>
  <% end %>
</div>  